{
    "dimensions": {
        "wheel_base": 0.6,

        "max_velocity": 1.1,
        "max_acceleration": 0.5,
        "max_angular_velocity": 1,
        "max_angular_acceleration": 0.5,
        "drive_resolution": 0.731635938e-3
    },

    "logging" : {
        "version": 1,
        "formatters": {
            "default": {
                "format": "%(asctime)s %(filename)s:%(lineno)d: %(message)s",
                "datefmt": "%Y-%m-%d %H:%M:%S"
            }
        },
        "handlers": {
            "stderr": {
                "class": "logging.StreamHandler",
                "formatter": "default",
                "level": "DEBUG",
                "stream": "ext://sys.stderr"
            }
        },
        "root": {
            "level": "DEBUG",
            "handlers": [ "stderr" ]
        },
        "disable_existing_loggers": false
    }
}
